{
  "preset": "ts-jest/presets/js-with-babel-esm",
  "coveragePathIgnorePatterns": [
    "/node_modules/"
  ], 
  "transformIgnorePatterns": [
    "!node_modules/"
  ],
  "testPathIgnorePatterns": [
    "/__tests__/fixtures.ts"
  ],
  "moduleNameMapper": {
    "./src/(.*?).js": "./$1.ts"
  },
  "verbose": true
}